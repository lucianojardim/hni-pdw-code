@using PDWModels.SpecRequests
@model SpecRequestInformation.FileListing

		@if( Model != null && Model.FileList.Any() )
  {
	  <div>
	  @foreach( var item in Model.FileList )
   {
	   <a href="/Documents/Typicals/@Model.SpecName/@item.Extension/@item.VersionNumber/@item.FileName" target="_blank">@item.FileName</a> <text>(@item.UploadDate)</text><br />
   }
	  </div>
  }
